{
  "name": "express-ts-starter",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "SET DEBUG=app:* && node ./dist/app.js",
    "build-dist": "rimraf dist/ && tsc && cpy views/*.* dist/ --parents && cpy public/**/*.* dist/ --parents",
    "devenv-win": "npm run watch-async-win & npm run supervise",
    "postinstall": "typings install",
    "supervise": "SET DEBUG=app:* && supervisor -w dist -e ts -x node ./dist/app.js",
    "watch-async-win": "start \"watch-async\" /b tsc -w",
    "stop-watch-win": "taskkill /FI \"IMAGENAME eq cmd.exe\" /F"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "cpy-cli": "^1.0.1",
    "rimraf": "^2.5.2",
    "supervisor": "^0.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
  }
}
